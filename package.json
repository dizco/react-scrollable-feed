    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss-modules": "2.0.1",
    "rollup-plugin-typescript2": "^0.21.2",
    "rollup-plugin-url": "^2.2.4",
    "typescript": "^3.9.9"
  },
  "files": [
    "dist"
  ]
